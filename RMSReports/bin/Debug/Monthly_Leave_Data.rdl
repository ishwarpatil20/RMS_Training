    <DataSet Name="EmpCode">
      <Query>
        <DataSourceName>RMSDataSource</DataSourceName>
        <QueryParameters>
          <QueryParameter Name="@EmpID">
            <Value>=Parameters!EmpID.Value</Value>
          </QueryParameter>
        </QueryParameters>
        <CommandText>select distinct b.EMPCode , b.FirstName + ' ' + b.LastName 'EmpName', b.Probation_Date, 0 'Paid_Leave', 0 'Casual_Leave'
from T_Employees  a inner join T_Employees_Hist b on a.EMPId = b.EMPId 
where a.EMPId in (select EMPId from T_Employees where EMPCode =@EmpID)
	and (b.Probation_Date is not null OR b.EMPCode like '%C%' OR 	b.Probation_Date != (select Probation_Date from T_Employees where EMPCode ='P2270'))
union 
select distinct a.EMPCode, a.FirstName + ' ' + a.LastName 'EmpName', a.Probation_Date, a.Paid_Leave, a.Casual_Leave
from T_Employees  a inner join T_Employees_Hist b on a.EMPId = b.EMPId 
where a.EMPId in (select EMPId from T_Employees where EMPCode =@EmpID)</CommandText>
      </Query>
      <Fields>
        <Field Name="EMPCode">
          <DataField>EMPCode</DataField>
          <rd:TypeName>System.String</rd:TypeName>
        </Field>
